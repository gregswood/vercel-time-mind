{
  "name": "time-mind",
  "version": "1.0.0",
  "description": "This project builds a time tracking app for the browser, allowing a user to start and stop timers throughout the day.",
  "source": "src/index.html",
  "scripts": {
    "dev": "parcel --no-cache --https",
    "clean": "rimraf dist",
    "build": "npm run clean && parcel build --log-level verbose",
    "setup": "npm ci",
    "postinstall": "husky install",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vercel/node": "^1.12.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "parcel": "^2.0.1",
    "postcss": "^8.3.11",
    "postcss-custom-properties": "^12.0.0",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-url": "^10.1.3",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "src/**/*.{ts,json}": [
      "prettier --write",
      "npm run lint:fix"
    ]
  }
}
